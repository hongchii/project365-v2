<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.mapper.QuestionMapper">
	<!-- com.project.domain.QuestionVO -->
	<select id="getQuestion" parameterType="int"
		resultType="com.project.domain.QuestionVO">
		select question_num, question from QUESTION WHERE
		question_num = #{question_num}
	</select>

</mapper>